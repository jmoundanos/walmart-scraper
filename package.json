{
  "name": "walmart-scraper",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmoundanos/walmart-scraper.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jmoundanos/walmart-scraper/issues"
  },
  "homepage": "https://github.com/jmoundanos/walmart-scraper#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.3.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongodb": "^3.6.0",
    "mongoose": "^5.10.0",
    "nodemon": "^2.0.4",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {},
  "description": ""
}
